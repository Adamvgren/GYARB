script/source = "extends CharacterBody2D

@onready var anim: AnimatedSprite2D = $AnimatedSprite2D

@export var speed: float = 60.0
@export var move_distance: float = 200.0
@export var direction: Vector2 = Vector2.UP  

var start_position: Vector2
var moving_forward = true

func _ready():
	start_position = global_position
	direction = direction.normalized()
	anim.play(\"MOVE\")  

func _physics_process(delta):
	var target_offset = direction * move_distance
	var target_pos = start_position + (target_offset if moving_forward else Vector2.ZERO)

	
	global_position = global_position.move_toward(target_pos, speed * delta)

	
	if global_position.distance_to(target_pos) < 1.0:
		moving_forward = !moving_forward




















\"\"\"
extends AnimatableBody2D

@onready var anim: AnimatedSprite2D = $AnimatedSprite2D

@export var speed: float = 60
@export var travel_distance: float = 200

var top_y: float
var bottom_y: float
var direction := -1 

func _ready():
	bottom_y = global_position.y
	top_y = bottom_y - travel_distance
	anim.play(\"MOVE\")

func _process(delta):
	global_position.y += speed * direction * delta

	if global_position.y <= top_y:
		global_position.y = top_y
		direction = 1
	elif global_position.y >= bottom_y:
		global_position.y = bottom_y
		direction = -1
\"\"\"
